{
  "compilerOptions": {
    "lib": [
      "es2021",
      "dom",
      "dom.iterable"
    ],
    "module": "es2020",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "importsNotUsedAsValues": "error"
  },
  "exclude": [
    "loading/vite",
    "loading/webworker-umd",
    "loading/webworker-webpack",
    "node_modules",
    "toolkit/angular",
    "toolkit/react-typescript",
    "toolkit/svelte",
    "toolkit/vue2-cli-typescript",
    "toolkit/vue3-cli-typescript"
  ]
}
